{
    "name": "magetest/magento-behat-extension",
    "description": "Magento Behat extension",
    "type": "library",
    "keywords": ["behat","bdd","magento"],
    "homepage": "https://github.com/MageTest/BehatMage",
    "license": "MIT",
    "repositories": {
        "inviqa/magento-community": {
            "type": "vcs",
            "url": "https://github.com/inviqa/magento-community.git"
        }
    },
    "require": {
        "php": "^5.6 || ^7.0",
        "behat/behat": "^3.0.15",
        "behat/mink-extension": "~2",
        "behat/mink-goutte-driver": "~1.1.0"
    },
    "require-dev": {
        "phpspec/phpspec": "^3.4",
        "mockery/mockery": "*",
        "inviqa/magento-community": "*"
    },
    "autoload": {
        "psr-0": {
            "":[
                "vendor/inviqa/magento-community/app",
                "vendor/inviqa/magento-community/lib",
                "vendor/inviqa/magento-community/app/code/local",
                "vendor/inviqa/magento-community/app/code/community",
                "vendor/inviqa/magento-community/app/code/core"
            ],
            "MageTest": "src/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Alistair Stead",
            "email": "astead@sessiondigital.com"
        },
        {
            "name": "Marcello Duarte",
            "email": "mduarte@sessiondigital.com"
        },
        {
            "name": "Sarunas Valaskevicius",
            "email": "svalaskevicius@sessiondigital.com"
        },
        {
            "name": "Daniel Kidanemariam",
            "email": "dkidanemariam@sessiondigital.com"
        },
        {
            "name": "Mark Slocock",
            "email": "mark@gpmd.co.uk"
        },
        {
            "name": "Marco De Bortoli",
            "email": "marco.debortoli@sessiondigital.com"
        },
        {
            "name": "Other contributors",
            "homepage": "https://github.com/MageTest/BehatMage/contributors"
        }
    ],
    "extra": {
        "magento-root-dir": "vendor/inviqa/magento-community"
    }
}
